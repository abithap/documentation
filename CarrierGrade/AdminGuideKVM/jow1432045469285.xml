  <title id="513">Customer Logs Reference Table</title>
  <shortdesc id="514"> The Customer Logs include events that do not require immediate user
    action.</shortdesc>
  <prolog>
    <author id="515">Jim Owens</author>
  </prolog>
  <refbody>
    <section id="section_N1001F_N1001C_N10001" id="516">
      <p id="517">The following types of events are included in the Customer Logs. The
        severity of the events is represented in the table by one or more letters, as follows:</p>
      <p id="504"/>
      <ul id="ul_jsd_jkg_vp">
        <li id="505">
          <p id="506">C: Critical</p>
        </li>
        <li id="507">
          <p id="508">M: Major</p>
        </li>
        <li id="509">
          <p id="510">m: Minor</p>
        </li>
        <li id="511">
          <p id="512">W: Warning</p>
        </li>
      </ul>
      <table id="table_vkl_3gy_bs">
        <tgroup cols="4">
          <colspec colname="logid" colwidth="15"/>
          <colspec colname="reason" colwidth="40"/>
          <colspec colname="entity" colwidth="40"/>
          <colspec colname="severity" colwidth="15"/>
          <thead>
            <row>
              <entry colname="logid" id="518">
                <p id="519">Log ID</p>
              </entry>
              <entry colname="reason" id="520">
                <p id="521">Reason</p>
              </entry>
              <entry colname="entity" id="522">
                <p id="523">Entity Instance ID</p>
              </entry>
              <entry colname="severity" id="524">
                <p id="525">Severity</p>
              </entry>
            </row>
          </thead>
          <tbody>
            <row>
              <entry id="526">
                <p id="527">700.002</p>
              </entry>
              <entry id="528">
                <p id="529">Instance STOP command issued</p>
              </entry>
              <entry id="530">
                <p id="531">instance=&lt;<varname id="532">instance_uuid</varname>&gt;</p>
              </entry>
              <entry id="533">
                <p id="534">C</p>
              </entry>
            </row>
            <row>
              <entry id="535">
                <p id="536">700.004</p>
              </entry>
              <entry id="537">
                <p id="538">Instance PAUSE command issued</p>
              </entry>
              <entry id="539">
                <p id="540">instance=&lt;<varname id="541">instance_uuid</varname>&gt;</p>
              </entry>
              <entry id="542">
                <p id="543">C</p>
              </entry>
            </row>
            <row>
              <entry id="544">
                <p id="545">700.005</p>
              </entry>
              <entry id="546">
                <p id="547">Instance SUSPEND command issued</p>
              </entry>
              <entry id="548">
                <p id="549">instance=&lt;<varname id="550">instance_uuid</varname>&gt;</p>
              </entry>
              <entry id="551">
                <p id="552">C</p>
              </entry>
            </row>
            <row>
              <entry id="553">
                <p id="554">700.011</p>
              </entry>
              <entry id="555">
                <p id="556">Instance DELETE command issued</p>
              </entry>
              <entry id="557">
                <p id="558">instance=&lt;<varname id="559">instance_uuid</varname>&gt;</p>
              </entry>
              <entry id="560">
                <p id="561">C</p>
              </entry>
            </row>
            <row>
              <entry id="562">
                <p id="563">700.012</p>
              </entry>
              <entry id="564">
                <p id="565">Instance was not able to cold or live migrate</p>
              </entry>
              <entry id="566">
                <p id="567">instance=&lt;<varname id="568">instance_uuid</varname>&gt;</p>
              </entry>
              <entry id="569">
                <p id="570">C</p>
              </entry>
            </row>
          </tbody>
        </tgroup>
      </table>
    </section>
  </refbody>
</reference-wr>